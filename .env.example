# Copy this file to .env and fill in your values

# HuggingFace token for Pyannote speaker diarization (optional but recommended)
# Get your token from: https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=

# UnRAID permissions (default values work for most setups)
PUID=99
PGID=100

# Directory mappings (adjust to your needs)
INPUT_DIR=./input
OUTPUT_DIR=./output
CACHE_DIR=./cache
MODELS_DIR=./models